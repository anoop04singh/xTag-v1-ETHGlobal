datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id                 String @id @default(cuid())
  nfcId              String @unique
  walletAddress      String @unique
  encryptedSignerKey String
}

model NfcTestMapping {
  id         String @id @default(cuid())
  nfcId      String @unique
  privateKey String
}